issn = {1526-5501},
journal = {Management Science},
keywords = {attribute substitution,heuristics,hospitals,operational performance},
number = {6},
pages = {2545--2563},
title = {{Heuristic Thinking in Patient Care}},
url = {http://pubsonline.informs.org.https//doi.org/10.1287/mnsc.2019.3332http://www.informs.orghttp://pubsonline.informs.org/journal/mnschttp://orcid.org/0000-0003-4358-2058},

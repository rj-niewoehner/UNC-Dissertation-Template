journal = {Stochastic Systems},
keywords = {EDA,Queueing,Queueing models,emergency departments,event logs,healthcare,hospital wards,patient flow,queueing networks},
mendeley-tags = {Queueing},
number = {1},
pages = {146--194},
title = {{On patient flow in hospitals: A data-based queueing-science perspective}},
volume = {5},
year = {2015}
}

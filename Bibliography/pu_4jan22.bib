file = {:C\:/Users/rniewoeh/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Gopalakrishnan et al. - 2016 - Routing and Staffing When Servers Are Strategic.pdf:pdf},
issn = {1526-5463},
journal = {Operations Research},
keywords = {Queueing,games/group decisions: noncooperative Area of revi,limit theorem,routing,scheduling,service systems,staffing,strategic servers Subject classifications: queues:},
mendeley-tags = {Queueing},

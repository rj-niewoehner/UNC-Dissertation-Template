journal = {Operations Research (forthcoming)},
keywords = {Behavioral Operations Management,Customer Ownership,Game Theory,Queueing,Queueing Theory,Server Pooling},
mendeley-tags = {Queueing},
title = {{Pooling Queues with Strategic Servers: The Effects of Customer Ownership}},
url = {https://ssrn.com/abstract=2951959},
